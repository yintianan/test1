<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="homework2.ParamMapper">

	


	<insert id="insertParam" parameterType="homework2.ParamInfo"
		useGeneratedKeys="true">
		insert into ParamInfo(methodid,paramIndex,paramType) values
		(#{methodId},#{paramIndex},#{paramType})
	</insert>
</mapper>